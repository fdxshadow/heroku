	<div class="container">
    <div class="row">
    <div class="well">
        <div class="list-group" id="productos">
        

        <% for(var i=0; i < productos.length; i++) {%>
          <a class="list-group-item">
              <div class="media col-md-3">
                    <figure class="pull-left">
                        <img class="media-object img-rounded img-responsive" src="http://placehold.it/350x250" alt="placehold.it/350x250">
                    </figure>
              </div>      
                <div class="col-md-6">
                    <h3 class="list-group-item-heading"><strong><%=productos[i].nombreproducto%></strong></h3>
                    <p class="list-group-item-text"><%=productos[i].descripcion%></p>
                </div>
                <div class="col-md-3 text-center">
                    <h2> <%=productos[i].precio%> <small> Precio </small></h2>
                    <input type="hidden" id="nombre" value="<%= productos[i].nombreproducto%>">
                    <input type="hidden" id="precio" value="<%= productos[i].precio%>">
                    <input type="hidden" id="id_p" value="<%= productos[i].id%>">
                    <input type="hidden" id="id_u" value="<%=session.User.id%>">
                    <button type="button" class="btn btn-primary btn-lg btn-block"><span class="glyphicon glyphicon-plus" aria-hidden="true"></span> Agregar al carrito</button>
                </div>
           </a>

           <%}%>
    


        </div>
        </div>
  </div>
</div>